import java.util.*;
public class project_2 {

	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		int n;
		n=s.nextInt();
		int year,days;
		int x=60*24*365;
		year=n/x;
		System.out.print(year+" years ");
		n=n-(year*x);
		int y=60*24;
		days=n/y;
		System.out.print("and "+days+" days.");
	}
}
